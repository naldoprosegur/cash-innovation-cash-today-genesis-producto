       SUM(MPD.NUM_IMPORTE) NUM_IMPORTE,
       MPD.COD_TIPO_MEDIO_PAGO || DIV.OID_DIVISA OID_TIPO_MEDIO_PAGO_DIVISA,
       '1' ES_TOTAL
FROM RPGE_YDOCUMENTO D
INNER JOIN RPGE_YMEDIO_PAGOXDOCUMENTO MPD ON MPD.OID_DOCUMENTO = D.OID_DOCUMENTO
INNER JOIN RPGE_YDIVISA DIV ON DIV.OID_DIVISA = MPD.OID_DIVISA
WHERE MPD.COD_NIVEL_DETALLE = 'T' AND D.COD_COMPROBANTE = :P_COD_COMPROBANTE
GROUP BY MPD.COD_TIPO_MEDIO_PAGO,DIV.OID_DIVISA,DIV.COD_ISO_DIVISA,DIV.DES_DIVISA
UNION
SELECT decode(MPD.COD_TIPO_MEDIO_PAGO,'codtipoa','Tickets','codtipob','Cheque','codtipoc','Tarjetas','codtipo','Otros Valores')as des_valor,
       DIV.OID_DIVISA,
       DIV.COD_ISO_DIVISA, 
       DIV.DES_DIVISA,
       MP.OID_MEDIO_PAGO,
       MP.COD_MEDIO_PAGO,
       MP.DES_MEDIO_PAGO,
       SUM(MPD.NEL_CANTIDAD) CANTIDAD,
       SUM(MPD.NUM_IMPORTE) NUM_IMPORTE,
       MPD.COD_TIPO_MEDIO_PAGO || DIV.OID_DIVISA OID_TIPO_MEDIO_PAGO_DIVISA,
       '0' ES_TOTAL
FROM RPGE_YDOCUMENTO D
INNER JOIN RPGE_YMEDIO_PAGOXDOCUMENTO MPD ON MPD.OID_DOCUMENTO = D.OID_DOCUMENTO
INNER JOIN RPGE_YDIVISA DIV ON DIV.OID_DIVISA = MPD.OID_DIVISA
INNER JOIN RPGE_YMEDIO_PAGO MP ON MP.OID_MEDIO_PAGO = MPD.OID_MEDIO_PAGO
WHERE MPD.COD_NIVEL_DETALLE = 'D' AND D.COD_COMPROBANTE = :P_COD_COMPROBANTE
GROUP BY MPD.COD_TIPO_MEDIO_PAGO,DIV.OID_DIVISA,DIV.COD_ISO_DIVISA,DIV.DES_DIVISA, MP.OID_MEDIO_PAGO,MP.COD_MEDIO_PAGO,MP.DES_MEDIO_PAGO</CommandText>
