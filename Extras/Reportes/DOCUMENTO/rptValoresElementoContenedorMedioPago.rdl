    <DataSet Name="ds_declarado_medio_pago">
      <Query>
        <DataSourceName>Reporte</DataSourceName>
        <QueryParameters>
          <QueryParameter Name=":P_COD_COMPROBANTE">
            <Value>=Parameters!P_COD_COMPROBANTE.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT  

  CASE WHEN BULTO.TIPO_ELEMENTO IS NOT NULL THEN BULTO.TIPO_ELEMENTO 
       WHEN PARCIAL.TIPO_ELEMENTO IS NOT NULL THEN PARCIAL.TIPO_ELEMENTO
  END TIPO_ELEMENTO,

  CASE WHEN BULTO.DOCUMENTO IS NOT NULL THEN BULTO.DOCUMENTO 
       WHEN PARCIAL.DOCUMENTO IS NOT NULL THEN PARCIAL.DOCUMENTO
  END DOCUMENTO,
  
  CASE WHEN BULTO.OID_TIPO_MEDIO_PAGO_DIVISA IS NOT NULL THEN BULTO.OID_TIPO_MEDIO_PAGO_DIVISA 
       WHEN PARCIAL.OID_TIPO_MEDIO_PAGO_DIVISA IS NOT NULL THEN PARCIAL.OID_TIPO_MEDIO_PAGO_DIVISA
  END OID_TIPO_MEDIO_PAGO_DIVISA,
  
  CASE WHEN BULTO.IMPORTE IS NOT NULL THEN BULTO.IMPORTE 
       WHEN PARCIAL.IMPORTE IS NOT NULL THEN PARCIAL.IMPORTE
  END IMPORTE

FROM

  (SELECT 'G' TIPO_ELEMENTO,DOC.OID_DOCUMENTO DOCUMENTO,DMP.COD_TIPO_MEDIO_PAGO,DMP.OID_DIVISA OID_DIVISA,
        SUM(DMP.NEL_CANTIDAD),SUM(DMP.NUM_IMPORTE) IMPORTE,DMP.COD_TIPO_MEDIO_PAGO || DMP.OID_DIVISA OID_TIPO_MEDIO_PAGO_DIVISA
       FROM RPGE_YDOCUMENTO DOC
       INNER JOIN RPGE_YDOCUMENTOXELEMENTO DE ON DE.OID_DOCUMENTO = DOC.OID_DOCUMENTO
       INNER JOIN RPGE_YDECLARADO_MEDIO_PAGO DMP ON DMP.OID_REMESA = DE.OID_REMESA
       WHERE DOC.COD_COMPROBANTE = :P_COD_COMPROBANTE AND DMP.OID_BULTO IS NOT NULL 
       GROUP BY DOC.OID_DOCUMENTO,DMP.COD_TIPO_MEDIO_PAGO,DMP.OID_DIVISA ) GERAL

LEFT JOIN

  (SELECT 'P' TIPO_ELEMENTO,DOC.OID_DOCUMENTO DOCUMENTO,DMP.COD_TIPO_MEDIO_PAGO,DMP.OID_DIVISA OID_DIVISA,
        SUM(DMP.NEL_CANTIDAD),SUM(DMP.NUM_IMPORTE) IMPORTE,DMP.COD_TIPO_MEDIO_PAGO || DMP.OID_DIVISA OID_TIPO_MEDIO_PAGO_DIVISA
       FROM RPGE_YDOCUMENTO DOC
       INNER JOIN RPGE_YDOCUMENTOXELEMENTO DE ON DE.OID_DOCUMENTO = DOC.OID_DOCUMENTO
       INNER JOIN RPGE_YDECLARADO_MEDIO_PAGO DMP ON DMP.OID_REMESA = DE.OID_REMESA
       WHERE DE.OID_BULTO IS NOT NULL 
       AND DMP.OID_BULTO IS NOT NULL 
       AND DMP.OID_PARCIAL IS NOT NULL
       AND DOC.COD_COMPROBANTE = :P_COD_COMPROBANTE
       GROUP BY DOC.OID_DOCUMENTO,DMP.COD_TIPO_MEDIO_PAGO,DMP.OID_DIVISA ) PARCIAL ON GERAL.OID_TIPO_MEDIO_PAGO_DIVISA = PARCIAL.OID_TIPO_MEDIO_PAGO_DIVISA
       
LEFT JOIN

 (SELECT 'B' TIPO_ELEMENTO,DOC.OID_DOCUMENTO DOCUMENTO,DMP.COD_TIPO_MEDIO_PAGO,DMP.OID_DIVISA OID_DIVISA,
        SUM(DMP.NEL_CANTIDAD),SUM(DMP.NUM_IMPORTE) IMPORTE,DMP.COD_TIPO_MEDIO_PAGO || DMP.OID_DIVISA OID_TIPO_MEDIO_PAGO_DIVISA
       FROM RPGE_YDOCUMENTO DOC
       INNER JOIN RPGE_YDOCUMENTOXELEMENTO DE ON DE.OID_DOCUMENTO = DOC.OID_DOCUMENTO
       INNER JOIN RPGE_YDECLARADO_MEDIO_PAGO DMP ON DMP.OID_BULTO = DE.OID_BULTO
       WHERE DE.OID_BULTO IS NOT NULL 
       AND DMP.OID_BULTO IS NOT NULL 
       AND DMP.OID_PARCIAL IS NULL
       AND DOC.COD_COMPROBANTE = :P_COD_COMPROBANTE
       GROUP BY DOC.OID_DOCUMENTO,DMP.COD_TIPO_MEDIO_PAGO,DMP.OID_DIVISA ) BULTO ON GERAL.OID_TIPO_MEDIO_PAGO_DIVISA = BULTO.OID_TIPO_MEDIO_PAGO_DIVISA</CommandText>
      </Query>
